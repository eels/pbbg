{
  "name": "@pbbg/api",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "copy:environment": "ln -sf  \"../../.env\" \"./\"",
    "dev": "onchange --initial --kill \"./src/**/*.{js,json,ts}\" -- yarn start",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./src/**/}*.{js,json,ts}\" --check",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint \"./*.{js,json,ts}\" \"./src/**/*.{js,json,ts}\" --cache",
    "prepare": "yarn copy:environment",
    "start": "ts-node --files \"./src/index.ts\" --transpileOnly",
    "test": "jest --passWithNoTests --setupFiles dotenv/config",
    "typescript:verify": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "1.1.1",
    "@pbbg/http": "*",
    "@pbbg/utilities": "*",
    "@pbbg/validators": "*",
    "awilix": "8.0.1",
    "change-case": "4.1.2",
    "date-fns": "2.30.0",
    "hono": "3.6.3",
    "jose": "4.14.6",
    "pocketbase": "0.15.2",
    "rate-limiter-flexible": "3.0.0",
    "timestring": "7.0.0",
    "validator": "13.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@pbbg/database-types": "*",
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/typescript-config": "*",
    "@types/jest": "29.5.3",
    "@types/node": "20.3.2",
    "@types/timestring": "6.0.2",
    "@types/validator": "13.7.17",
    "dotenv": "16.0.0",
    "jest": "29.6.1",
    "node-mocks-http": "1.12.2",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "ts-toolbelt": "9.6.0",
    "tsc-alias": "1.8.6",
    "tsconfig-paths": "4.2.0"
  }
}
