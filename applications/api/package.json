{
  "name": "@pbbg/api",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "environment:copy": "ln -sf  \"../../.env\" \"./\"",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./src/**/}*.{js,json,ts}\" --check",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint \"./*.{js,json,ts}\" \"./src/**/*.{js,json,ts}\" --cache",
    "prepare": "yarn environment:copy",
    "test": "jest --passWithNoTests --setupFiles dotenv/config",
    "typescript:verify": "tsc --noEmit"
  },
  "dependencies": {
    "@pbbg/http": "*",
    "@pbbg/utilities": "*",
    "@pbbg/validators": "*",
    "awilix": "8.0.1",
    "axios": "1.4.0",
    "change-case": "4.1.2",
    "date-fns": "2.30.0",
    "express-rate-limit": "6.7.0",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "next-auth": "4.22.1",
    "pocketbase": "0.15.2",
    "timestring": "7.0.0",
    "validator": "13.9.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@pbbg/database-types": "*",
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/typescript-config": "*",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "20.3.2",
    "@types/timestring": "6.0.2",
    "@types/validator": "13.7.17",
    "dotenv": "16.0.0",
    "jest": "29.6.1",
    "node-mocks-http": "1.12.2",
    "rimraf": "3.0.2",
    "ts-toolbelt": "9.6.0"
  }
}
