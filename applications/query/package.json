{
  "name": "@pbbg/query",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rimraf \"./build\"",
    "dev:client": "ts-node --files \"./src/index.ts\" --transpileOnly",
    "dev": "onchange --initial --kill \"./src/**/*.{js,json,ts}\" -- yarn dev:client",
    "copy:environment": "ln -sf  \"../../.env\" \"./\"",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./src/**/}*.{js,json,ts}\" --check",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint \"./*.{js,json,ts}\" \"./src/**/*.{js,json,ts}\" --cache",
    "prepare": "yarn copy:environment",
    "start": "node \"./build/index.js\"",
    "test": "jest --passWithNoTests --setupFiles dotenv/config",
    "typescript:verify": "tsc --noEmit"
  },
  "dependencies": {
    "@pbbg/http": "*",
    "@pbbg/utilities": "*",
    "awilix": "8.0.1",
    "dotenv": "16.0.0",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "sqlite": "4.2.1",
    "sqlite3": "5.1.6"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/typescript-config": "*",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "20.3.2",
    "jest": "29.6.1",
    "onchange": "7.1.0",
    "rimraf": "5.0.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.6",
    "tsconfig-paths": "4.2.0"
  }
}
