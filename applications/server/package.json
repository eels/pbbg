{
  "name": "@pbbg/server",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf build logs",
    "dev": "nodemon --exec 'ts-node src/index.ts --transpileOnly' --ext '*' --watch './src/**'",
    "format": "prettier '{.,./{src,test}/**/}*.{js,json,ts,tsx}' --check",
    "format:fix": "yarn format --write",
    "lint": "yarn lint:js",
    "lint:js": "eslint './*.{js,ts}' './{src,test}/**/*.{js,ts,tsx}' --cache",
    "lint:js:fix": "yarn lint:js --fix",
    "test": "jest --cache --passWithNoTests",
    "test:coverage": "yarn test --coverage"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@pbbg/database-service": "*",
    "@prisma/client": "^3.8.1",
    "@sentry/node": "^6.17.4",
    "body-parser": "^1.19.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.2.1",
    "jose": "^4.5.0",
    "morgan": "^1.10.0",
    "winston": "^3.5.1"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*",
    "typescript": "*",
    "webpack": "*"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/typescript-config": "*",
    "@swc/core": "^1.2.141",
    "@swc/jest": "^0.2.17",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.12",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
