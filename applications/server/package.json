{
  "name": "@pbbg/server",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "node ace build --production",
    "clean": "rimraf build",
    "dev": "node ace serve --watch",
    "environment:copy": "ln -sf  ../../.env .",
    "format": "prettier \"{.,./{src,test}/**/}*.{js,json,ts}\" --check",
    "format:fix": "yarn format --write",
    "lint": "yarn lint:js",
    "lint:js": "eslint \"./*.{js,ts}\" \"./{src,test}/**/*.{js,ts}\" --cache",
    "lint:js:fix": "yarn lint:js --fix",
    "prepare": "yarn environment:copy",
    "start": "node server.js",
    "test": "node ace test",
    "typescript:verify": "tsc --noEmit"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.2.2",
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/limiter": "^1.0.1",
    "@adonisjs/lucid": "^18.1.0",
    "@adonisjs/redis": "^7.3.1",
    "@adonisjs/repl": "^3.1.0",
    "luxon": "^3.0.3",
    "pg": "^8.8.0",
    "phc-argon2": "^1.1.3",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.8.1",
    "@japa/preset-adonis": "^1.1.1",
    "@japa/runner": "^2.1.1",
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/typescript-config": "*",
    "@types/node": "^17.0.12",
    "@types/proxy-addr": "^2.0.0",
    "@types/source-map-support": "^0.5.6",
    "adonis-preset-ts": "^2.1.0",
    "pino-pretty": "^9.1.0",
    "rimraf": "^3.0.2",
    "ts-toolbelt": "^9.6.0",
    "youch": "^3.2.1",
    "youch-terminal": "^2.1.5"
  }
}
