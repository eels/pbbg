{
  "name": "@pbbg/web-client",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next && rimraf out",
    "create:post": "yarn ts-node scripts/create-blog-post --transpileOnly",
    "create:component": "yarn ts-node scripts/create-component --transpileOnly",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier './{.,scripts,src,storybook,test}/**/*.{js,json,ts,tsx}' --check",
    "format:fix": "yarn format --write",
    "lint": "yarn lint:js && yarn lint:css",
    "lint-staged": "lint-staged",
    "lint:css": "stylelint './src/**/*.scss' --cache",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:js": "eslint './{.,scripts,src,storybook,test}/**/*.{js,ts,tsx}' --cache",
    "lint:js:fix": "yarn lint:js --fix",
    "storybook:build": "build-storybook --config-dir storybook --quiet",
    "storybook:start": "start-storybook --config-dir storybook --port 6006 --quiet",
    "test": "jest --passWithNoTests",
    "test:coverage": "yarn test --coverage"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "a11y-react-emoji": "^1.2.0",
    "change-case": "^4.1.2",
    "chic-modules": "^1.0.7",
    "date-fns": "^2.28.0",
    "next": "^12.0.9",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "prettier": "*",
    "webpack": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/stylelint-config": "*",
    "@pbbg/typescript-config": "*",
    "@storybook/addon-a11y": "^6.4.18",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-interactions": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "eslint": "^8.7.0",
    "inquirer": "^8.2.0",
    "gray-matter": "^4.0.3",
    "jest": "^27.4.7",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.2",
    "next-purge-css-modules": "^1.0.1",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "stylelint": "^14.3.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}
