{
  "name": "@pbbg/web-client",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "yarn build:resources && yarn build:client && yarn build:sw",
    "build:client": "next build",
    "build:resources": "yarn ts-node scripts/create-static-resources --transpileOnly",
    "build:sw": "webpack",
    "clean": "rimraf .next out",
    "create:post": "yarn ts-node scripts/create-blog-post --transpileOnly",
    "create:component": "yarn ts-node scripts/create-component --transpileOnly",
    "dev": "yarn build:resources && next dev",
    "export": "next export && yarn export:images",
    "export:images": "yarn ts-node scripts/create-webp-images --transpileOnly",
    "format": "prettier '{.,./{scripts,src,storybook,test}/**/}*.{js,json,ts,tsx}' --check",
    "format:fix": "yarn format --write",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint './src/**/*.scss' --cache",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:js": "eslint './*.{js,ts}' './{scripts,src,storybook,test}/**/*.{js,ts,tsx}' --cache",
    "lint:js:fix": "yarn lint:js --fix",
    "storybook:build": "build-storybook --config-dir storybook --quiet",
    "storybook:start": "start-storybook --config-dir storybook --port 6006 --quiet",
    "test": "jest --cache --passWithNoTests",
    "test:coverage": "yarn test --coverage"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "a11y-react-emoji": "^1.2.0",
    "change-case": "^4.1.2",
    "chic-modules": "^1.0.7",
    "date-fns": "^2.28.0",
    "js-cookie": "^3.0.1",
    "next": "^12.1.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "workbox-cacheable-response": "^6.4.2",
    "workbox-expiration": "^6.4.2",
    "workbox-routing": "^6.4.2",
    "workbox-strategies": "^6.4.2"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*",
    "stylelint": "*",
    "typescript": "*",
    "webpack": "*",
    "webpack-cli": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/stylelint-config": "*",
    "@pbbg/typescript-config": "*",
    "@storybook/addon-a11y": "^6.4.18",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-interactions": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/html-minifier": "^4.0.2",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/sharp": "^0.29.5",
    "babel-loader": "^8.2.3",
    "critters": "^0.0.16",
    "dotenv": "^16.0.0",
    "gray-matter": "^4.0.3",
    "html-minifier": "^4.0.0",
    "inquirer": "^8.2.0",
    "jest": "^27.4.7",
    "jsdom": "^19.0.0",
    "markdown-it": "^12.3.2",
    "markdown-it-attrs": "^4.1.3",
    "next-purge-css-modules": "^1.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "sharp": "^0.30.1",
    "storybook-addon-next": "^1.4.4",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0"
  }
}
