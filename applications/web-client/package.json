{
  "name": "@pbbg/web-client",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "clean": "rimraf .next",
    "dev": "next dev",
    "export": "next export",
    "format": "prettier './{src,test}/**/*.{ts,tsx}' --check",
    "format:fix": "yarn format --write",
    "lint-staged": "lint-staged",
    "lint:css": "stylelint './src/**/*.scss' --cache",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:js": "eslint './{src,test}/**/*.{ts,tsx}' --cache",
    "lint:js:fix": "yarn lint:js --fix",
    "test": "jest --coverage --passWithNoTests"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "chic-modules": "^1.0.7",
    "next": "^12.0.9",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/stylelint-config": "*",
    "@pbbg/typescript-config": "*",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.13",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "eslint": "^8.7.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.2",
    "next-purge-css-modules": "^1.0.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "stylelint": "^14.3.0",
    "typescript": "^4.5.5"
  }
}
