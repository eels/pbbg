{
  "name": "@pbbg/web",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "yarn create:icon-type && yarn build:resources && yarn build:client && yarn build:sw",
    "build:client": "next build",
    "build:resources": "ts-node scripts/create-static-resources --transpileOnly",
    "build:sw": "webpack",
    "clean": "rimraf .next out",
    "create:post": "ts-node scripts/create-blog-post --transpileOnly",
    "create:component": "ts-node scripts/create-component --transpileOnly",
    "create:icon-type": "ts-node scripts/create-icon-type --transpileOnly",
    "dev": "yarn create:icon-type && yarn build:resources && next dev",
    "environment:copy": "ln -sf  ../../.env .",
    "export": "yarn build && yarn export:client && yarn export:images",
    "export:client": "next export",
    "export:images": "ts-node scripts/create-webp-images --transpileOnly",
    "format": "prettier \"{.,./{public,scripts,src,storybook,test}/**/}*.{css,js,json,svg,ts,tsx}\" --check",
    "format:fix": "yarn format --write",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint \"./src/**/*.{css,scss}\" --cache",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:js": "eslint \"./*.{js,ts}\" \"./{scripts,src,storybook,test}/**/*.{js,ts,tsx}\" --cache",
    "lint:js:fix": "yarn lint:js --fix",
    "optimise:svg": "yarn svgo --folder \"./public/icons\" --output \"./public/icons\"",
    "postbuild": "yarn sitemap:generate",
    "prepare": "yarn environment:copy",
    "sitemap:generate": "next-sitemap && yarn sitemap:minify",
    "sitemap:minify": "html-minifier \"./public/sitemap.xml\" --output \"./public/sitemap.xml\" --collapse-whitespace",
    "storybook:build": "build-storybook --config-dir storybook --quiet",
    "storybook:start": "start-storybook --config-dir storybook --port 6006 --quiet",
    "test": "jest --cache --passWithNoTests",
    "test:a11y": "start-server-and-test \"next dev\" 3000 \"pa11y-ci --sitemap http://localhost:3000/sitemap.xml\"",
    "test:coverage": "yarn test --coverage",
    "typescript:verify": "tsc --noEmit"
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "a11y-react-emoji": "^1.2.0",
    "axios": "^0.27.2",
    "change-case": "^4.1.2",
    "date-fns": "^2.29.2",
    "gray-matter": "^4.0.3",
    "html-minifier": "^4.0.0",
    "js-cookie": "^3.0.1",
    "markdown-it": "^12.3.2",
    "markdown-it-attrs": "^4.1.3",
    "next": "^12.3.0",
    "next-i18next": "^12.0.1",
    "next-sitemap": "^3.1.24",
    "no-try": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^1.3.0",
    "workbox-cacheable-response": "^6.5.4",
    "workbox-expiration": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/stylelint-config": "*",
    "@pbbg/typescript-config": "*",
    "@prettier/multiline-arrays@npm:prettier-plugin-multiline-arrays": "^1.1.1",
    "@storybook/addon-a11y": "^6.4.18",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-interactions": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/html-minifier": "^4.0.2",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^17.0.13",
    "@types/react-dom": "^18.0.6",
    "@types/react": "^18.0.19",
    "@types/sharp": "^0.29.5",
    "@types/svgo": "^2.6.4",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.3",
    "critters": "^0.0.16",
    "dotenv": "^16.0.0",
    "inquirer": "^8.2.0",
    "jest": "^27.4.7",
    "jsdom": "^19.0.0",
    "pa11y-ci": "^3.0.1",
    "postcss": "^8.4.16",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "sharp": "^0.30.1",
    "start-server-and-test": "^1.14.0",
    "storybook-addon-next": "^1.4.4",
    "svgo": "^2.8.0",
    "tailwind-compose": "^0.7.1",
    "tailwindcss": "^3.1.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "webpack-cli": "^4.9.2",
    "webpack": "^5.69.1"
  }
}
