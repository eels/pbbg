{
  "name": "@pbbg/web",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "node --require dotenv/config \"./node_modules/.bin/astro\" build",
    "clean": "rimraf \"./.astro\" \"./dist\"",
    "dev": "astro dev",
    "copy:environment": "ln -sf \"../../.env\" \"./\"",
    "copy:fonts": "cp -a  \"../../assets/fonts\" \"./public/\"",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./{public,src}/**/}*.{astro,js,json,mjs,svg,ts,tsx}\" --check",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint \"./*.{js,json,mjs,ts}\" \"./src/**/*.{astro,js,json,mjs,ts,tsx}\" --cache",
    "prepare": "yarn copy:environment && yarn copy:fonts",
    "start": "node --require dotenv/config \"./node_modules/.bin/astro\" preview",
    "typescript:verify": "tsc --noEmit"
  },
  "dependencies": {
    "@astrojs/node": "6.0.0",
    "@astrojs/react": "3.0.2",
    "@astrojs/tailwind": "5.0.0",
    "@pbbg/content": "*",
    "@pbbg/database-types": "*",
    "@pbbg/http": "*",
    "@pbbg/styles": "*",
    "@pbbg/ui": "*",
    "@pbbg/utilities": "*",
    "@pbbg/validators": "*",
    "astro-compress": "2.0.15",
    "astro-critters": "2.0.11",
    "astro": "3.1.0",
    "astrojs-service-worker": "1.1.0",
    "classcat": "5.0.4",
    "react-dom": "18.2.0",
    "react": "18.2.0",
    "tailwind-compose": "1.1.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/stylelint-config": "*",
    "@pbbg/tailwind-config": "*",
    "@prettier/multiline-arrays@npm:prettier-plugin-multiline-arrays": "1.1.1",
    "@types/node": "20.3.2",
    "@types/react-dom": "18.2.6",
    "@types/react": "18.2.14",
    "axios": "1.4.0",
    "prettier-plugin-astro": "0.8.0",
    "rimraf": "5.0.1",
    "sass": "1.49.0",
    "ts-toolbelt": "9.6.0"
  }
}
