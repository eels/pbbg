{
  "name": "@pbbg/web",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "next build",
    "clean": "rimraf \"./.next\" \"./.swc\"",
    "dev": "next dev",
    "environment:copy": "ln -sf  \"../../.env\" \"./\"",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./{public,src,test}/**/}*.{js,json,svg,ts,tsx}\" --check",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint \"./*.{js,json,ts}\" \"./{src,test}/**/*.{js,json,ts,tsx}\" --cache",
    "postbuild": "yarn sitemap:generate && yarn sitemap:minify",
    "prepare": "yarn environment:copy",
    "sitemap:generate": "next-sitemap",
    "sitemap:minify": "html-minifier \"./public/sitemap.xml\" --output \"./public/sitemap.xml\" --collapse-whitespace",
    "test:a11y": "start-server-and-test \"next dev\" 3000 \"pa11y-ci --sitemap http://localhost:3000/sitemap.xml\"",
    "test": "jest --passWithNoTests",
    "typescript:verify": "tsc --noEmit"
  },
  "dependencies": {
    "@pbbg/api": "*",
    "@pbbg/content": "*",
    "@pbbg/database-types": "*",
    "@pbbg/http": "*",
    "@pbbg/ui": "*",
    "@pbbg/utilities": "*",
    "@pbbg/validators": "*",
    "classcat": "5.0.4",
    "next-auth": "4.22.1",
    "next-contentlayer": "0.3.1",
    "next-sitemap": "4.1.3",
    "next": "13.4.7",
    "react-dom": "18.2.0",
    "react": "18.2.0",
    "tailwind-compose": "1.1.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/stylelint-config": "*",
    "@pbbg/tailwind-config": "*",
    "@pbbg/typescript-config": "*",
    "@prettier/multiline-arrays@npm:prettier-plugin-multiline-arrays": "1.1.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react": "14.0.0",
    "@types/html-minifier": "4.0.2",
    "@types/jest": "29.5.3",
    "@types/jsdom": "16.2.14",
    "@types/node": "20.3.2",
    "@types/react-dom": "18.2.6",
    "@types/react": "18.2.14",
    "@types/sharp": "0.29.5",
    "autoprefixer": "10.4.8",
    "critters": "0.0.16",
    "dotenv": "16.0.0",
    "html-minifier": "4.0.0",
    "jest-environment-jsdom": "29.6.1",
    "jest": "29.6.1",
    "jsdom": "19.0.0",
    "pa11y-ci": "3.0.1",
    "postcss": "8.4.22",
    "rimraf": "3.0.2",
    "sass": "1.49.0",
    "sharp": "0.30.1",
    "start-server-and-test": "1.14.0",
    "ts-toolbelt": "9.6.0"
  }
}
