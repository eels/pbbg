{
  "name": "@pbbg/web",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build:client": "next build",
    "build:icon-type": "ts-node --files \"./scripts/create-icon-type\" --transpileOnly",
    "build": "yarn build:icon-type && yarn build:client",
    "clean": "rimraf .next out",
    "create:component": "ts-node --files \"./scripts/create-component\" --transpileOnly",
    "create:post": "ts-node --files \"./scripts/create-blog-post\" --transpileOnly",
    "dev": "next dev",
    "environment:copy": "ln -sf  \"../../.env\" \"./\"",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./{public,scripts,src,storybook,test}/**/}*.{css,js,json,svg,ts,tsx}\" --check",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:css": "stylelint \"./src/**/*.{css,scss}\" --cache",
    "lint:fix": "yarn lint:js:fix && yarn lint:css:fix",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:js": "eslint \"./*.{js,json,ts}\" \"./{scripts,src,storybook,test}/**/*.{js,json,ts,tsx}\" --cache",
    "lint": "yarn lint:js && yarn lint:css",
    "optimise:svg": "yarn svgo --folder \"./public/icons\" --output \"./public/icons\"",
    "postbuild": "yarn sitemap:generate",
    "prepare": "yarn environment:copy",
    "sitemap:generate": "next-sitemap && yarn sitemap:minify",
    "sitemap:minify": "html-minifier \"./public/sitemap.xml\" --output \"./public/sitemap.xml\" --collapse-whitespace",
    "storybook:build": "storybook build --config-dir \"./storybook\" --quiet",
    "storybook:start": "storybook dev --config-dir \"./storybook\" --port 6006 --quiet",
    "test:a11y": "start-server-and-test \"next dev\" 3000 \"pa11y-ci --sitemap http://localhost:3000/sitemap.xml\"",
    "test:coverage": "yarn test --coverage",
    "test": "jest --cache --passWithNoTests",
    "typescript:verify": "tsc --noEmit"
  },
  "dependencies": {
    "@pbbg/http": "*",
    "awilix": "8.0.1",
    "axios": "1.4.0",
    "change-case": "4.1.2",
    "date-fns": "2.30.0",
    "express-rate-limit": "6.7.0",
    "express": "4.18.2",
    "gray-matter": "4.0.3",
    "helmet": "7.0.0",
    "html-minifier": "4.0.0",
    "js-cookie": "3.0.5",
    "markdown-it-attrs": "4.1.6",
    "markdown-it": "13.0.1",
    "next-auth": "4.22.1",
    "next-i18next": "14.0.0",
    "next-sitemap": "4.1.3",
    "next": "13.4.7",
    "no-try": "3.1.0",
    "pocketbase": "0.15.2",
    "react-dom": "18.2.0",
    "react": "18.2.0",
    "timestring": "7.0.0"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/stylelint-config": "*",
    "@pbbg/typescript-config": "*",
    "@prettier/multiline-arrays@npm:prettier-plugin-multiline-arrays": "1.1.1",
    "@storybook/addon-a11y": "7.0.26",
    "@storybook/addon-actions": "7.0.26",
    "@storybook/addon-essentials": "7.0.26",
    "@storybook/addon-interactions": "7.0.26",
    "@storybook/addon-links": "7.0.26",
    "@storybook/addon-styling": "1.3.2",
    "@storybook/blocks": "7.0.26",
    "@storybook/cli": "7.0.26",
    "@storybook/nextjs": "7.0.26",
    "@storybook/react": "7.0.26",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/react": "12.1.2",
    "@types/express": "4.17.17",
    "@types/html-minifier": "4.0.2",
    "@types/inquirer": "8.2.0",
    "@types/jest": "27.4.0",
    "@types/jsdom": "16.2.14",
    "@types/markdown-it-attrs": "4.1.0",
    "@types/markdown-it": "12.2.3",
    "@types/node": "20.3.2",
    "@types/react-dom": "18.2.6",
    "@types/react": "18.2.14",
    "@types/sharp": "0.29.5",
    "@types/svgo": "2.6.4",
    "@types/timestring": "6.0.2",
    "autoprefixer": "10.4.8",
    "critters": "0.0.16",
    "dotenv": "16.0.0",
    "inquirer": "8.2.0",
    "jest": "27.4.7",
    "jsdom": "19.0.0",
    "pa11y-ci": "3.0.1",
    "postcss": "8.4.22",
    "rimraf": "3.0.2",
    "sass": "1.49.0",
    "sharp": "0.30.1",
    "start-server-and-test": "1.14.0",
    "storybook": "7.0.26",
    "svgo": "3.0.2",
    "tailwind-compose": "0.7.1",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "ts-toolbelt": "9.6.0",
    "tsconfig-paths": "4.2.0"
  }
}
