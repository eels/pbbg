  "files": [
    "lib/*"
  ],
  "dependencies": {
    "pocketbase": "0.15.2"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/typescript-config": "*"
  }
}
