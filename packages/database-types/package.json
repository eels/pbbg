{
  "name": "@pbbg/database-types",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./lib/**/}*.{js,json,ts}\" --cache --check",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint \"./*.{js,ts}\" \"./lib/**/*.{js,ts}\" --cache",
    "typescript:verify": "tsc --noEmit"
