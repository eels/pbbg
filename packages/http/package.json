{
  "name": "@pbbg/http",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rimraf \"./lib\"",
    "dev": "onchange --initial \"./src/**/*.{js,json,ts}\" -- yarn build",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./src/**/}*.{js,json,ts}\" --check",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint \"./*.{js,json,ts}\" \"./src/**/*.{json,ts}\" --cache",
    "test": "jest --passWithNoTests",
    "typescript:verify": "tsc --noEmit",
    "wait": "wait-on \"./lib\""
  },
  "files": [
    "express-http.d.ts",
    "lib/*"
  ],
  "dependencies": {
    "axios": "1.4.0",
    "express": "4.18.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/typescript-config": "*",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "20.3.2",
    "jest": "29.6.1",
    "onchange": "7.1.0",
    "rimraf": "3.0.2",
    "ts-jest": "29.1.1",
    "tsc-alias": "1.8.6",
    "wait-on": "7.0.1"
  }
}
