{
  "name": "@pbbg/http",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "tsc --project \"./tsconfig.build.json\" && tsc-alias",
    "clean": "rimraf \"./lib\"",
    "dev": "onchange --initial \"./src/**/*.{js,json,ts}\" -- yarn build",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./src/**/}*.{cjs,js,json,ts}\" --check",
    "lint:fix": "yarn lint --fix",
    "lint": "eslint \"./*.{cjs,js,json,ts}\" \"./src/**/*.{json,ts}\" --cache",
    "test": "jest --passWithNoTests",
    "typescript:verify": "tsc --noEmit",
    "wait": "wait-on \"./lib\""
  },
  "dependencies": {
    "@pbbg/utilities": "*",
    "axios": "1.4.0",
    "hono": "3.6.3",
    "timestring": "7.0.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/typescript-config": "*",
    "@types/jest": "29.5.3",
    "@types/node": "20.3.2",
    "jest": "29.6.1",
    "jose": "4.14.6",
    "rate-limiter-flexible": "3.0.0",
    "ts-jest": "29.1.1"
  }
}
