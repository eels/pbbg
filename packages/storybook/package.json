{
  "name": "@pbbg/storybook",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "build": "yarn pre && storybook build --config-dir \"./src\" --quiet",
    "clean": "rimraf \"./src/--styles\" \"./storybook-static\"",
    "copy:styles": "cp -a \"../styles/lib\" \"./src/--styles\"",
    "dev": "yarn pre && storybook dev --config-dir \"./src\" --no-open --port 6006 --quiet",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./src/**/}*.{cjs,js,json,ts}\" --check",
    "lint:fix": "yarn lint:js --fix",
    "lint": "eslint \"./*.{cjs,js,json}\" \"./src/**/*.{json,ts}\" --cache",
    "pre": "yarn copy:styles",
    "typescript:verify": "tsc --noEmit"
  },
  "dependencies": {
    "@pbbg/styles": "*",
    "@pbbg/ui": "*",
    "@storybook/addon-a11y": "7.0.26",
    "@storybook/addon-actions": "7.0.26",
    "@storybook/addon-essentials": "7.0.26",
    "@storybook/addon-interactions": "7.0.26",
    "@storybook/addon-links": "7.0.26",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/blocks": "7.0.26",
    "@storybook/cli": "7.0.26",
    "@storybook/manager-api": "7.0.26",
    "@storybook/nextjs": "7.0.26",
    "@storybook/react": "7.0.26",
    "@storybook/theming": "7.0.26",
    "next": "13.4.19",
    "react-dom": "18.2.0",
    "react": "18.2.0",
    "storybook": "7.0.26"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/tailwind-config": "*",
    "@pbbg/typescript-config": "*",
    "@types/node": "20.3.2",
    "@types/react-dom": "18.2.6",
    "@types/react": "18.2.14",
    "autoprefixer": "10.4.8",
    "postcss": "8.4.22",
    "sass": "1.49.0"
  }
}
