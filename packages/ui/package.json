{
  "name": "@pbbg/ui",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "yarn create:icon-type",
    "clean": "rimraf \"./storybook-static\"",
    "create:component": "ts-node --files \"./scripts/create-component\" --transpileOnly",
    "create:icon-type": "ts-node --files \"./scripts/create-icon-type\" --transpileOnly",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./{scripts,src,storybook,test}/**/}*.{css,js,json,svg,ts,tsx}\" --check",
    "lint:css:fix": "yarn lint:css --fix",
    "lint:css": "stylelint \"./src/**/*.{css,scss}\" --cache",
    "lint:fix": "yarn lint:js:fix && yarn lint:css:fix",
    "lint:js:fix": "yarn lint:js --fix",
    "lint:js": "eslint \"./*.{js,json,ts}\" \"./{scripts,src,storybook,test}/**/*.{js,json,ts,tsx}\" --cache",
    "lint": "yarn lint:js && yarn lint:css",
    "optimise:svg": "yarn svgo --folder \"./src/resources/icons\" --output \"./src/resources/icons\"",
    "storybook:build": "storybook build --config-dir \"./storybook\" --quiet",
    "storybook:start": "storybook dev --config-dir \"./storybook\" --no-open --port 6006 --quiet",
    "test": "jest --passWithNoTests",
    "typescript:verify": "tsc --noEmit"
  },
  "files": [
    "src/*",
    "next-string.d.ts"
  ],
  "dependencies": {
    "@pbbg/database-types": "*",
    "@pbbg/utilities": "*",
    "@pbbg/validators": "*",
    "axios": "1.4.0",
    "change-case": "4.1.2",
    "classcat": "5.0.4",
    "inquirer": "8.2.0",
    "jsdom": "19.0.0",
    "next-auth": "4.22.1",
    "next": "13.4.7",
    "react-dom": "18.2.0",
    "react": "18.2.0",
    "tailwind-compose": "1.1.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/stylelint-config": "*",
    "@pbbg/tailwind-config": "*",
    "@pbbg/typescript-config": "*",
    "@prettier/multiline-arrays@npm:prettier-plugin-multiline-arrays": "1.1.1",
    "@storybook/addon-a11y": "7.0.26",
    "@storybook/addon-actions": "7.0.26",
    "@storybook/addon-essentials": "7.0.26",
    "@storybook/addon-interactions": "7.0.26",
    "@storybook/addon-links": "7.0.26",
    "@storybook/addon-styling": "1.3.2",
    "@storybook/blocks": "7.0.26",
    "@storybook/cli": "7.0.26",
    "@storybook/manager-api": "7.0.26",
    "@storybook/nextjs": "7.0.26",
    "@storybook/react": "7.0.26",
    "@storybook/testing-library": "0.2.0",
    "@storybook/theming": "7.0.26",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/react": "14.0.0",
    "@types/inquirer": "8.2.0",
    "@types/jest": "29.5.3",
    "@types/jsdom": "16.2.14",
    "@types/node": "20.3.2",
    "@types/react-dom": "18.2.6",
    "@types/react": "18.2.14",
    "@types/svgo": "2.6.4",
    "autoprefixer": "10.4.8",
    "jest-environment-jsdom": "29.6.1",
    "jest": "29.6.1",
    "postcss": "8.4.22",
    "rimraf": "3.0.2",
    "storybook": "7.0.26",
    "svgo": "3.0.2",
    "ts-node": "10.9.1",
    "ts-toolbelt": "9.6.0",
    "tsconfig-paths": "4.2.0"
  }
}
