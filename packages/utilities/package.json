{
  "name": "@pbbg/utilities",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "tsc && tsc-alias",
    "clean": "rimraf \"./lib/**/*.js\"",
    "format:fix": "yarn format --write",
    "format": "prettier \"{.,./lib/**/}*.{js,json,ts}\" --check",
    "lint:fix": "yarn lint:js --fix",
    "lint": "eslint \"./*.{js,json,ts}\" \"./lib/**/*.{json,ts}\" --cache",
    "typescript:verify": "tsc --noEmit"
  },
  "files": [
    "lib/*"
  ],
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/typescript-config": "*",
    "rimraf": "3.0.2",
    "tsc-alias": "1.8.6"
  }
}
