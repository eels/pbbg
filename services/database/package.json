{
  "name": "@pbbg/database-service",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "build": "yarn generate",
    "create:migration": "prisma migrate dev --name",
    "deploy": "prisma migrate deploy",
    "format": "prettier '{.,./prisma/**/}*.{js,json,ts}' --check",
    "format:fix": "yarn format --write && prisma format",
    "lint": "yarn lint:js",
    "lint-staged": "lint-staged",
    "lint:js": "eslint './*.{js,ts}' './prisma/**/*.{js,ts}' --cache",
    "lint:js:fix": "yarn lint:js --fix",
    "generate": "prisma generate",
    "prepare": "ln -sf  ../../.env .",
    "reset": "prisma migrate reset",
    "seed": "prisma db seed"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "prisma": "^3.8.1"
  },
  "peerDependencies": {
    "eslint": "*",
    "lint-staged": "*",
    "prettier": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "@pbbg/eslint-config": "*",
    "@pbbg/prettier-config": "*",
    "@pbbg/typescript-config": "*",
    "@types/node": "^17.0.12",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0"
  }
}
